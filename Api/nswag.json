{
  "runtime": "Net80",
  "documentGenerator": {
    "fromDocument": {
      "url": "swagger.json",
      "output": null,
      "newLineBehaviour": "Auto"
    }
  },
  "codeGenerators": {
    "openApiToCSharpClient": {
      "className": "Client",
      "namespace": "ApiClient.Generated",
      "output": "../Api.Client/Autogenerated.cs",
      "jsonLibrary": "SystemTextJson"
    }
  }
}